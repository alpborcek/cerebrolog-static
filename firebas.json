{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/main.dart.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/flutter.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/assets/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/payWallforNewUser",
        "destination": "/index.html"
      },
      {
        "source": "/payWall",
        "destination": "/index.html"
      },
      {
        "source": "/payWallEdu",
        "destination": "/index.html"
      },
      {
        "source": "/privacySecurity",
        "destination": "/index.html"
      },
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml"
      },
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "/signUp",
        "destination": "/index.html"
      },
      {
        "source": "/signIn",
        "destination": "/index.html"
      },
      {
        "source": "/payment-fail",
        "destination": "/index.html"
      },
      {
        "source": "/payment-success",
        "destination": "/index.html"
      },
      {
        "source": "/cerebroEduStudentConsent",
        "destination": "/index.html"
      },
      {
        "source": "/cerebroEduSupervisorConsent",
        "destination": "/index.html"
      },
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  }
}
